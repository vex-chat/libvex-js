{
    "defaultSeverity": "error",
    "jsRules": {},
    "rules": {
        "quotemark": [true, "double"],
        "no-console": [true, "never"],
        "object-literal-sort-keys": false,
        "trailing-comma": [
            true,
            {
                "multiline": {
                    "objects": "always",
                    "arrays": "always",
                    "functions": "never"
                },
                "esSpecCompliant": true
            }
        ],
        "no-default-export": true
    },
    "rulesDirectory": [],
    "extends": ["tslint:latest", "tslint-config-prettier"]
}
